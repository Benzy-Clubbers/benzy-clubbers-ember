<BsCarousel
  @nextControlIcon="glyphicon glyphicon-chevron-right"
  @nextControlLabel="Suivant"
  @prevControlIcon="glyphicon glyphicon-chevron-left"
  @prevControlLabel="Précédent" as |car|
  >
  {{#each @images as |image|}}
     <car.slide>
    <img src={{image.picture_url}} class="d-block w-100" >
  </car.slide>
{{/each}}
  
</BsCarousel>